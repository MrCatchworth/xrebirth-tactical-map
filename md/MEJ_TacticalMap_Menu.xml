<?xml version="1.0" encoding="UTF-8"?>
<mdscript name="MEJ_TacticalMap_Menu" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="md.xsd">
    <cues>
        <cue name="MeJ_OpenTacticalMap" instantiate="true" namespace="this">
            <conditions>
                <check_any>
                    <event_conversation_next_section section="MeJ_OpenTacticalMap"/>
                    <event_conversation_returned_to_section section="MeJ_OpenTacticalMap"/>
                </check_any>
            </conditions>
            <actions>
                <open_conversation_menu menu="MeJ_TacticalMapMenu" />
                <add_conversation_view view="closeupdetailmonitor"/>
            </actions>
        </cue>
    </cues>
</mdscript>